A,CAAA,QAAA,CAAA,OAAA,EAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,UAAA,GAAA,GAAA,QAAA,CAAA,OAAA,CAAA,CAAA,EAAA,GAAA;AACA,UAAA,KAAA,CAAA,GAAA,EAAA;AACA,YAAA,MAAA,EAAA;AADA,WAAA,CAAA,CAGA,IAHA,CAGA,UAAA,QAAA,EAAA;AACA,mBAAA,QAAA,CAAA,IAAA,EAAA;AACA,WALA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,gBAAA,IAAA,GAAA,MAAA,QAAA,CAAA,IAAA,GAAA,GAAA,GAAA,IAAA,CAAA,SAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,GAAA;AACA,YAAA,KAAA,CAAA,IAAA,CAAA;;AACA,YAAA,MAAA,CAAA,SAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,WAVA;AAWA;AACA,OAvBA;AAwBA,KArDA;AAsDA,IAAA,YAtDA,wBAsDA,CAtDA,EAsDA;AACA,WAAA,MAAA,GAAA,CAAA;AACA,WAAA,aAAA;AACA,KAzDA;AA0DA,IAAA,UA1DA,sBA0DA,CA1DA,EA0DA;AACA,WAAA,IAAA,GAAA,CAAA;AACA,WAAA,aAAA;AACA,KA7DA;AA8DA,IAAA,aA9DA,yBA8DA,CA9DA,EA8DA;AACA,WAAA,UAAA,GAAA,CAAA;AACA;AAhEA;AAzCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <Top @search=\"updateSearch\" @page=\"updatePage\"/>\r\n    <Stats :id=\"Number(stats_show)\" :color_types=\"tipos_cor\"/>\r\n    <List :name_list=\"name_list\" :color_types=\"tipos_cor\" :type_list=\"type_list\" :n_pokemons=\"n_pokemons\" :id_list=\"id_list\" :search=\"search\" :page=\"page\" @page=\"updatePage\" @statsID=\"updateStatsID\" /> \r\n     \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Top from './components/Top.vue'\r\nimport List from './components/List.vue'\r\nimport Stats from './components/Stats.vue'\r\n\r\nexport default {\r\n  name: 'app',\r\n  components: {\r\n    Top,\r\n    List,\r\n    Stats\r\n  },\r\n  data(){\r\n    return{\r\n      pokemons: Object,\r\n      page: 0,\r\n      search: '',\r\n      limit: 807,\r\n      offset: (this.page*this.limit),\r\n      id_list: [],\r\n      name_list: [],\r\n      type_list: [],\r\n      typeSearch: String,\r\n      n_pokemons: 0,\r\n      stats_show: 1,\r\n      tipos_cor: {bug: \"#A8B820\", dark: \"#705848\", dragon: \"#7038F8\",\r\n                  electric: \"#F8D030\", fairy: \"#EE99AC\", fighting: \"#C03028\",\r\n                  flying: \"#A890F0\",ghost: \"#705898\",ground: \"#E0C068\",\r\n                  normal: \"#705848\",grass:\"#78C850\" , fire: \"#E24242\",\r\n                  poison: \"#A040A0\", psychic: \"#F85888\",rock: \"#B8A038\",\r\n                  steel: \"#B8B8D0\", ice:\"#98D8D8\", water:\"#6890F0\"}\r\n    }\r\n  },\r\n  mounted: function(){\r\n    this.getPokemonsTypes();\r\n    fetch('https://pokeapi.co/api/v2/pokemon/?limit='+this.limit+'&offset='+this.offset, {\r\n      method: 'get'\r\n    })\r\n    .then((response) =>{\r\n      return response.json()\r\n    })\r\n    .then((response) =>{\r\n      this.pokemons = response.results;\r\n      this.generate_list();\r\n    });\r\n  },\r\n  methods:{\r\n    getIDFromURL: function(url){\r\n        return (url.match(/(\\d+)/g) || [])[1];\r\n    },\r\n    generate_list(){\r\n      this.name_list = [];\r\n      this.id_list = [];\r\n      var i = 0;\r\n      if(this.search === ''){\r\n        for(i=this.page*60;i<(this.page+1)*60; i++){\r\n          this.name_list.push(this.pokemons[i].name);\r\n          this.id_list.push(this.getIDFromURL(this.pokemons[i].url));\r\n        }   \r\n        this.n_pokemons = this.limit \r\n      }\r\n      else{\r\n        this.n_pokemons = 0\r\n        for(i=0; (i < Object.keys(this.pokemons).length); i++){\r\n          if(this.pokemons[i].name.toUpperCase().includes(this.search.toUpperCase())){\r\n            this.n_pokemons++\r\n            if(this.n_pokemons < (60*(this.page+1)) && this.n_pokemons > (60*(this.page))){\r\n              this.name_list.push(this.pokemons[i].name);\r\n              this.id_list.push(this.getIDFromURL(this.pokemons[i].url));\r\n            }\r\n          }\r\n        }\r\n      }\r\n    },\r\n    getPokemonsTypes(){\r\n      fetch('https://pokeapi.co/api/v2/type', {\r\n        method: 'get'\r\n      })\r\n      .then((response) =>{\r\n        return response.json()\r\n      \r\n      })\r\n      .then((response) =>{\r\n        var url = \"\";\r\n        for(var i=0; i<Object.keys(response.results).length; i++){\r\n          url  = response.results[i].url\r\n          fetch(url, {\r\n            method: 'get'\r\n          })\r\n          .then((response) =>{\r\n            return response.json()\r\n          })\r\n          .then((response) =>{\r\n            var json = \"{\"+response.name+\":\"+ JSON.stringify(response.pokemon)+\"}\"\r\n            alert(json)\r\n            this.type_list.push(JSON.parse(json));  \r\n          });\r\n        }\r\n      });\r\n    },\r\n    updateSearch(s){\r\n      this.search = s;\r\n      this.generate_list();\r\n    },\r\n    updatePage(p){\r\n      this.page = p;\r\n      this.generate_list();\r\n    },\r\n    updateStatsID(s){\r\n      this.stats_show = s;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n@import \"font.css\"; \r\n\r\nbody{\r\n    overflow-x: auto;\r\n    background-color: #fff;\r\n}\r\n*{\r\n    margin: 0;\r\n    font: 20px 'Product Sans thin Regular';\r\n    border: 0px solid black;\r\n    transition: 0.3s;\r\n}\r\na {\r\n    color: inherit; /* blue colors for links too */\r\n    text-decoration: inherit; \r\n    font: inherit;\r\n}\r\n\r\n::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */\r\n    color: #aaa;\r\n    opacity: 1; /* Firefox */\r\n}\r\n</style>\r\n"],"sourceRoot":"src"}]}