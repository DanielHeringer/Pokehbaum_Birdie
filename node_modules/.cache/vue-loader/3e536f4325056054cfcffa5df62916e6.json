  },\r\n  data(){\r\n    return{\r\n      pokemons: Object,\r\n      page: 0,\r\n      search: '',\r\n      limit: 808,\r\n      offset: (this.page*this.limit),\r\n      url_list: [],\r\n      name_list: [],\r\n      stats_show: 1\r\n    }\r\n  },\r\n  mounted: function(){\r\n    fetch('https://pokeapi.co/api/v2/pokemon/?limit='+this.limit+'&offset='+this.offset, {\r\n      method: 'get'\r\n    })\r\n    .then((response) =>{\r\n      return response.json()\r\n    })\r\n    .then((response) =>{\r\n      this.pokemons = response.results;\r\n      this.generate_list();\r\n    });\r\n  },\r\n  methods:{\r\n    generate_list: function(){\r\n      this.name_list = [];\r\n      this.url_list = [];\r\n        var i = 0;\r\n        if(this.search === ''){\r\n          for(i=this.page*151;i<(this.page+1)*151; i++){\r\n            this.name_list.push(this.pokemons[i].name);\r\n            this.url_list.push(this.pokemons[i].url);\r\n          }   \r\n        }\r\n        else{\r\n          var flag = 1;\r\n          for(i=0; (i < Object.keys(this.pokemons).length) && flag; i++){\r\n            if(this.pokemons[i].name.toUpperCase().includes(this.search.toUpperCase())){\r\n              this.name_list.push(this.pokemons[i].name);\r\n              this.url_list.push(this.pokemons[i].url);\r\n            }\r\n            if(this.name_list.length > 151){\r\n              flag = 0;\r\n            }\r\n          }\r\n        }\r\n      },\r\n    updateSearch(s){\r\n      this.search = s;\r\n      this.generate_list();\r\n    },\r\n    updatePage(p){\r\n      this.page = p;\r\n      this.generate_list();\r\n    },\r\n    updateStatsID(s){\r\n      this.stats_show = s;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n@import \"font.css\"; \r\n\r\nbody{\r\n    overflow-x: auto;\r\n    background-color: #fff;\r\n}\r\n*{\r\n    margin: 0;\r\n    font: 20px 'Product Sans thin Regular';\r\n    border: 0px solid black;\r\n    transition: 0.3s;\r\n}\r\na {\r\n    color: inherit; /* blue c